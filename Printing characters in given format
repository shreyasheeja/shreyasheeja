/* Required output :

A 

A B 

A B C 

A B C D 

A B C D E 

A B C D E F 

A B C D E F G 

*/

CODE : 

  public class HelloWorld
{

     public static void main(String []args)
     {
        for (char i=65;i<=71;i++)     // ASCII value of A is 065
        {
            for (char j=65;j<=i;j++)
            {
                System.out.print(j + " ");
            }
            System.out.println ("\n");
        }
     }
}
